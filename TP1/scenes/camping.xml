<lsf>
    <initials>
        <reference length="2" />
        <!-- axis length; "0" means no axis displayed -->
        <root id="root" />
        <!-- identifier of root node of the scene graph -->
        <!-- this node must be defined in one of the following NODE declarations -->
    </initials>

    <views default="Camping Scene">
        <!-- At least one camera must be present. -->
        <!-- camera types : perspective and ortho -->
        
        <!-- view over all the scene -->
        <perspective id="Camping Scene" near="0.1" far="500" angle="17">
            <from x="30" y="50" z="80" />
            <to x="0" y="7" z="0" />
        </perspective>
        
        <!-- detailed view of a tree -->
        <ortho id="Tree View" near="-1" far="60" left="-3" right="3" top="4" bottom="-3">
            <from x="-3" y="2.5" z="4" />
            <to x="-4" y="1.5" z="5" />
            <up x="0" y="1" z="0" />    <!-- optional, default 0, 1, 0 -->
        </ortho>

        <!-- detailed view of the bonfire -->
        <perspective id="Bonfire View" near="0.1" far="10" angle="50">
            <from x="-1" y="3" z="3" />
            <to x="0" y="0" z="0" />
        </perspective>

        <!-- detailed view of a picnic -->
        <ortho id="Picnic Scene" near="-10" far="100" left="-1" right="1" top="1" bottom="-1">
            <from x="-6.7" y="7" z="-7.1" />
            <to x="-0.5" y="0" z="-6" />
            <up x="0" y="1" z="0" />    <!-- optional, default 0, 1, 0 -->
        </ortho>
    </views>

    <illumination>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />           <!-- global ambient -->
        <background r="0.0" g="0.0" b="0.0" a="1.0" />        <!-- background color -->
    </illumination>

    <lights>
        <!-- At least one light should be present, and a maximum of eight lights is accepted. -->
        
        <!-- Simulates the light provided by a fire -->
        <light id="Fire Light">                             <!-- light identifier -->
            <enable value="1" />                            <!-- enable/disable -->
            <position x="0" y="0" z="0" w="1.0" />          <!-- light position; w=1: point light; w=0: directional light -->
            <ambient  r="0.7" g="0.6" b="0.3" a="0.0" />    <!-- ambient component -->
            <diffuse  r="0.7" g="0.6" b="0.3" a="1.0" />    <!-- diffuse component -->
            <specular r="0.7" g="0.6" b="0.3" a="1.0" />    <!-- specular component -->
        </light>

        <!-- Simulates a full moon -->
        <light id="Full Moon">                              <!-- light identifier -->
            <enable value="0" />                            <!-- enable/disable -->
            <position x="3" y="10" z="3" w="1.0" />         <!-- light position; w=1: point light; w=0: directional light -->
            <ambient  r="0.5" g="0.5" b="0.5" a="1.0" />    <!-- ambient component -->
            <diffuse  r="0.6" g="0.6" b="0.6" a="1.0" />    <!-- diffuse component -->
            <specular r="0.6" g="0.6" b="0.6" a="1.0" />    <!-- specular component -->
        </light>

        <!-- Simulates a new moon -->
        <light id="New Moon">                               <!-- light identifier -->
            <enable value="0" />                            <!-- enable/disable -->
            <position x="-3" y="10" z="-3" w="1.0" />       <!-- light position; w=1: point light; w=0: directional light -->
            <ambient  r="0.5" g="0.5" b="0.5" a="1.0" />    <!-- ambient component -->
            <diffuse  r="0.2" g="0.2" b="0.2" a="1.0" />    <!-- diffuse component -->
            <specular r="0.2" g="0.2" b="0.2" a="1.0" />    <!-- specular component -->
        </light>

        <!-- Simulates the sun -->
        <light id="Sun">                                    <!-- light identifier -->
            <enable value="0" />                            <!-- enable/disable -->
            <position x="0.0" y="10" z="0.0" w="1.0" />     <!-- light position; w=1: point light; w=0: directional light -->
            <ambient  r="1.0" g="1.0" b="1.0" a="1.0" />    <!-- ambient component -->
            <diffuse  r="1.0" g="1.0" b="1.0" a="1.0" />    <!-- diffuse component -->
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />    <!-- specular component -->
        </light> 
        
    </lights>

    <textures>
        <texture id="logTexture"         path="./scenes/images/log.jpg" />
        <texture id="forestFloorTexture" path="./scenes/images/bottom.png" />
        <texture id="rocksTexture"       path="./scenes/images/rocks.jpg" />
        <texture id="coalTexture"        path="./scenes/images/coal.jpg" />
        <texture id="leavesTexture"      path="./scenes/images/leaves.jpg" />
