<lsf>
    <initials>
        <reference length="2" />
        <!-- axis length; "0" means no axis displayed -->
        <root id="root" />
        <!-- identifier of root node of the scene graph -->
        <!-- this node must be defined in one of the following NODE declarations -->
    </initials>

    <views default="Camping Scene">
        <!-- At least one camera must be present. -->
        <!-- camera types : perspective and ortho -->
        
        <!-- view over all the scene -->
        <perspective id="Camping Scene" near="0.1" far="500" angle="20">
            <from x="60" y="55" z="30" />
            <to x="0" y="-2" z="0" />
        </perspective>
        
        <!-- detailed view of a tree -->
        <ortho id="Tree View" near="-1" far="50" left="-3" right="3" top="3" bottom="-3">
            <from x="-3" y="3" z="3" />
            <to x="-4" y="2" z="4" />
            <up x="0" y="1" z="0" />    <!-- optional, default 0, 1, 0 -->
        </ortho>

        <!-- detailed view of the bonfire -->
        <perspective id="Bonfire View" near="0.1" far="10" angle="50">
            <from x="-1" y="3" z="3" />
            <to x="0" y="0" z="0" />
        </perspective>

        <!-- special view : inside of the tent -->
        <ortho id="Tent View" near="0" far="5" left="-0.5" right="0.5" top="0.5" bottom="-0.5">
            <from x="2.6" y="1.0" z="3.5" />
            <to x="3.9" y="0.2" z="0.0" />
            <up x="0" y="1" z="0" />    <!-- optional, default 0, 1, 0 -->
        </ortho>
    </views>

    <illumination>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />           <!-- global ambient -->
        <background r="0.0" g="0.0" b="0.0" a="1.0" />        <!-- background color -->
    </illumination>

    <lights>
        <!-- At least one light should be present, and a maximum of eight lights is accepted. -->
        
        <!-- Simulates the light provided by a fire -->
        <light id="Fire Light">                             <!-- light identifier -->
            <enable value="1" />                            <!-- enable/disable -->
            <position x="0" y="0" z="0" w="1.0" />          <!-- light position; w=1: point light; w=0: directional light -->
            <ambient  r="0.0" g="0.0" b="0.0" a="1.0" />    <!-- ambient component -->
            <diffuse  r="0.3" g="0.3" b="0.3" a="1.0" />    <!-- diffuse component -->
            <specular r="0.3" g="0.3" b="0.3" a="1.0" />    <!-- specular component -->
        </light>

        <!-- Simulates a full moon -->
        <light id="Full Moon">                              <!-- light identifier -->
            <enable value="0" />                            <!-- enable/disable -->
            <position x="3" y="10" z="3" w="1.0" />         <!-- light position; w=1: point light; w=0: directional light -->
            <ambient  r="0.0" g="0.0" b="0.0" a="1.0" />    <!-- ambient component -->
            <diffuse  r="0.6" g="0.6" b="0.6" a="1.0" />    <!-- diffuse component -->
            <specular r="0.6" g="0.6" b="0.6" a="1.0" />    <!-- specular component -->
        </light>

        <!-- Simulates a new moon -->
        <light id="New Moon">                               <!-- light identifier -->
            <enable value="0" />                            <!-- enable/disable -->
            <position x="-3" y="10" z="-3" w="1.0" />       <!-- light position; w=1: point light; w=0: directional light -->
            <ambient  r="0.0" g="0.0" b="0.0" a="1.0" />    <!-- ambient component -->
            <diffuse  r="0.2" g="0.2" b="0.2" a="1.0" />    <!-- diffuse component -->
            <specular r="0.2" g="0.2" b="0.2" a="1.0" />    <!-- specular component -->
        </light>

        <!-- Simulates the sun -->
        <light id="Sun">                                    <!-- light identifier -->
            <enable value="0" />                            <!-- enable/disable -->
            <position x="0" y="10" z="0" w="1.0" />         <!-- light position; w=1: point light; w=0: directional light -->
            <ambient  r="0.0" g="0.0" b="0.0" a="1.0" />    <!-- ambient component -->
            <diffuse  r="0.9" g="0.9" b="0.9" a="1.0" />    <!-- diffuse component -->
            <specular r="0.9" g="0.9" b="0.9" a="1.0" />    <!-- specular component -->
        </light>
    </lights>

    <textures>
        <texture id="logTexture"         path="./scenes/images/log.jpg" />
        <texture id="forestFloorTexture" path="./scenes/images/moss.jpg" />
        <texture id="rocksTexture"       path="./scenes/images/rocks.jpg" />
        <texture id="coalTexture"        path="./scenes/images/coal.jpg" />
        <texture id="leavesTexture"      path="./scenes/images/leaves.jpg" />
        <texture id="dirtTexture"        path="./scenes/images/dirt.jpg" />
    </textures>

    <materials>
        <!-- At least one material should be present. -->
        <material id="flamesMaterial">
            <shininess value="30" />
            <ambient  r="1.0" g="0.0" b="0.0" a="1.0" />
            <diffuse  r="0.7" g="0.4" b="0.0" a="1.0" />
            <specular r="0.7" g="0.7" b="0.0" a="1.0" />
            <emissive r="0.3" g="0.3" b="0.0" a="1.0" />
        </material>
        <material id="tentMaterial">
            <shininess value="10" />
            <ambient  r="0.6" g="0.3" b="0.0" a="1.0" />
            <diffuse  r="1.0" g="0.8" b="0.6" a="1.0" />
            <specular r="1.0" g="0.8" b="0.6" a="1.0" />
            <emissive r="0.0" g="0.0" b="0.0" a="1.0" />
        </material>
        <material id="rocksMaterial">
            <shininess value="1" />
            <ambient  r="0.6" g="0.6" b="0.6" a="1.0" />
            <diffuse  r="0.8" g="0.8" b="0.8" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <emissive r="0.0" g="0.0" b="0.0" a="1.0" />
        </material>
    </materials>

    <nodes>
        <node id="root">

            <material id="tentMaterial" />
            
            <texture id="clear">
                <amplification afs="2.0" aft="2.0" />
            </texture>

            <transformations>
            </transformations>

            <descendants>
                <noderef id="ground" />
                <noderef id="bonfire" />
                <noderef id="side_log"/>
                <noderef id="tent"/>
                <noderef id="trees"/>
            </descendants>

        </node>

        <node id="ground">
            
            <material id="null" />
            
            <texture id="clear">
                <amplification afs="1" aft="1" />
            </texture>

            <transformations>
                <rotation axis="x" angle="-90" />
            </transformations>

            <descendants>
                <noderef id="topGround" />
                <noderef id="bottomGround" />
                <noderef id="backGround" />
                <noderef id="frontGround" />
                <noderef id="leftGround" />
                <noderef id="rightGround" />
            </descendants>

        </node>

        <node id="topGround">

            <material id="null" />

            <texture id="forestFloorTexture">
                <amplification afs="1" aft="1" />
            </texture>

            <transformations>
            </transformations>

            <descendants>
                <leaf type="rectangle" x1="-10" y1="-10" x2="10" y2="10" />
            </descendants>

        </node>

