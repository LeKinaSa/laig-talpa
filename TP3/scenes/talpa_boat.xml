        <node id="scenario">
            <material id="default" />
            <texture id="null">
                <amplification afs="1" aft="1" />
            </texture>
            <transformations>
            </transformations>
            <descendants>
                <noderef id="surroundings" />
                <noderef id="river" />
                <noderef id="canoe" />
            </descendants>
        </node>

        <node id="surroundings">
            <material id="default" />
            <texture id="lake">
                <amplification afs="1" aft="1" />
            </texture>
            <transformations>
                <translation x="0" y="-15" z="0" />
                <rotation axis="y" angle="180" />
                <rotation axis="x" angle="90" />
            </transformations>

            <descendants>
                <leaf type="sphere" radius="-100" slices="32" stacks="32" />
            </descendants>
        </node>

        <node id="river">
